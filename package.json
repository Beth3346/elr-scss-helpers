{
  "name": "elr-scss-helpers",
  "description": "a scss mixin with some css helper classes",
  "version": "0.0.12",
  "author": "Elizabeth Rogers",
  "bugs": {
    "url": "https://github.com/Beth3346/elr-scss-helpers/issues"
  },
  "scripts": {
    "clean": "gulp clean",
    "build": "parcel build src/index.html",
    "serve": "parcel serve src/index.html",
